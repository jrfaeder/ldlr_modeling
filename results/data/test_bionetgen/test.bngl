
begin model
begin parameters
    k_on 1.0
end parameters
begin molecule types
    A(b)
    B(a)
end molecule types
begin seed species
    A(b) 100
    B(a) 100
end seed species
begin observables
    Molecules A_tot A()
end observables
begin reaction rules
    A(b) + B(a) -> A(b!1).B(a!1) k_on
end reaction rules
end model
generate_network({overwrite=>1})
simulate({method=>"ode", t_end=>10, n_steps=>10})
